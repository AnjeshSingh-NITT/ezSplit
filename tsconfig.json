{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(auth)/layout.tsx", "convex/schema.js", "convex/schema.js", "convex/users.js", "middleware.js", "convex/seed.js", "convex/contacts.js", "app/(main)/contacts/page.jsx", "app/(main)/contacts/page.jsx", "app/(main)/contacts/_components/create-group-modal.jsx", "hooks/use-convex-query.js", "hooks/use-store-user.jsx", "app/(auth)/sign-up/[[...sign-up]]/page.jsx", "app/(auth)/sign-in/[[...sign-in]]/page.jsx", "app/(main)/layout.jsx", "components/convex-client-provider.jsx", "app/page.tsx", "app/layout.tsx", "app/(main)/dashboard/page.jsx"],
  "exclude": ["node_modules"]
}
